  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "cross-env LOG_LEVEL=debug nodemon src/index.js --watch src",
    "debug": "cross-env LOG_LEVEL=debug nodemon --inspect=0.0.0.0:9229 src/index.js --watch src",
    "lint": "eslint \"./src/**/*.js\" \"tests/**/*.js\"",
    "test": "cross-env dotenv_config_path=env.jest node -r dotenv/config ./node_modules/jest/bin/jest.js --runInBand -c jest.config.js",
    "test:watch": "cross-env dotenv_config_path=env.jest node -r dotenv/config ./node_modules/jest/bin/jest.js --runInBand --watch -c jest.config.js",
    "coverage": "cross-env dotenv_config_path=env.jest node -r dotenv/config ./node_modules/jest/bin/jest.js --runInBand --coverage -c jest.config.js",
    "test:integration": "hurl --test --glob \"tests/integration/**/*.hurl\""
  },
  "devDependencies": {
    "@orangeopensource/hurl": "^7.0.0",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^7.4.1",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.940.0",
    "@aws-sdk/client-s3": "^3.934.0",
    "@aws-sdk/lib-dynamodb": "^3.940.0",
    "aws-jwt-verify": "^5.1.1",
    "bcrypt": "^6.0.0",
