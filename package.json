    "compression": "^1.7.5",
    "content-type": "^1.0.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "markdown-it": "^14.1.0",
    "passport": "^0.7.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "stoppable": "^1.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js"
